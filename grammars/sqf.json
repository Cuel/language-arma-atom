{
  "scopeName": "source.sqf",
  "name": "SQF",
  "fileTypes": [
    "sqf"
  ],
  "patterns": [
    {
      "name": "comment.line.double-slash.sqf",
      "match": "//.*$"
    },
    {
      "name": "comment.block.sqf",
      "begin": "comment (\"|')",
      "end": "\\1"
    },
    {
      "name": "comment.block.sqf",
      "begin": "/\\*",
      "end": "\\*/"
    },
    {
      "name": "constant.numeric.sqf",
      "match": "\\b(0x[a-fA-F\\d]+|\\d+(\\.\\d+)?(e[+-]?\\d+)?)|pi\\b"
    },
    {
      "name": "constant.language.sqf",
      "match": "\\b(?i:true|false|nil)\\b"
    },
    {
      "name": "keyword.control.sqf",
      "match": "\\b(?i:if|then|else|exitWith|while|do|for|from|to|with|switch|case|default|breakOut|breakTo|forEach|step)\\b"
    },
    {
      "name": "keyword.operator.arithmetic.sqf",
      "match": "\\+|-|\\*|/|%|>>|\\^"
    },
    {
      "name": "keyword.operator.assignment.sqf",
      "match": "="
    },
    {
      "name": "keyword.operator.comparison.sqf",
      "match": "<=|>=|==|<|>|!=|isEqualTo"
    },
    {
      "name": "keyword.operator.logical.sqf",
      "match": "\\b(?i:and|or|not)\\b|\\B[&\\|]{2}\\B|!"
    },
    {
      "name": "meta.preprocessor.c",
      "begin": "^\\s*(#)(define|undef|ifdef|ifndef|else|endif|include)\\b",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.keyword.c"
        },
        "2": {
          "name": "keyword.control.import.c"
        }
      },
      "end": "(?=(?://|/\\*))|(?<!\\\\)(?=\\n)",
      "patterns": [
        {
          "match": "(?>\\\\\\s*\\n)",
          "name": "punctuation.separator.continuation.c"
        }
      ]
    },
    {
      "name": "keyword.other.cba.sqf",
      "match": "(\\b|##)(ADDON|ARR_d|[QD]?E?FUNC|GET[MU]?VAR|Q?E?GVAR|PATHTOF|PATHTOEF|PREP|QUOTE|SET[MU]?VAR)(\\b|##)"
    },
    {
      "name": "string.single.sqf",
      "begin": "'",
      "end": "'"
    },
    {
      "name": "string.double.sqf",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "support.constant.sqf",
      "match": "\\b(?i:controlNull|displayNull|grpNull|locationNull|netObjNull|objNull|scriptNull|taskNull|teamMemberNull|west|east|resistance|civilian|independent|blufor|opfor)\\b"
